number = {12},
pages = {822--831},
publisher = {Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
title = {{Enabling the 'host jump': structural determinants of receptor-binding specificity in influenza A viruses}},
url = {http://dx.doi.org/10.1038/nrmicro3362 10.1038/nrmicro3362},
volume = {12},
year = {2014}
}
