issn = {1740-1526},
journal = {Nature reviews. Microbiology},
pages = {109--122},
pmid = {15043007},
title = {{Virus entry: molecular mechanisms and biomedical applications.}},
volume = {2},
year = {2004}
}
@misc{Neumann2006,
abstract = {Influenza A viruses cause pandemics at random intervals. Pandemics are caused by viruses that contain a hemagglutinin (HA) surface glycoprotein to which human populations are immunologically naive. Such an HA can be introduced into the human population through reassortment between human and avian virus strains or through the direct transfer of an avian influenza virus to humans. The factors that determine the interspecies transmission and pathogenicity of influenza viruses are still poorly understood; however, the HA protein plays an important role in overcoming the interspecies barrier and in virulence in avian influenza viruses. Recently, the RNA polymerase (PB2) protein has also been recognized as a critical factor in host range restriction, while the nonstructural (NS1) protein affects the initial host immune responses. We summarize current knowledge of viral factors that determine host range restriction and pathogenicity of influenza A viruses.},
author = {Neumann, Gabriele and Kawaoka, Yoshihiro},
booktitle = {Emerging Infectious Diseases},
doi = {10.3201/eid1206.051336},
file = {:Users/Masha/Library/Application Support/Mendeley Desktop/Downloaded/Neumann, Kawaoka - 2006 - Host range restriction and pathogenicity in the context of influenza pandemic.pdf:pdf},
isbn = {1080-6040 (Print)},
issn = {10806040},
pages = {881--886},
pmid = {16707041},
title = {{Host range restriction and pathogenicity in the context of influenza pandemic}},
volume = {12},
year = {2006}
}
@article{LeMay2004,
abstract = {The Rift Valley fever virus (RVFV) is the causative agent of fatal hemorrhagic fever in humans and acute hepatitis in ruminants. We found that infection by RVFV leads to a rapid and drastic suppression of host cellular RNA synthesis that parallels a decrease of the TFIIH transcription factor cellular concentration. Using yeast two hybrid system, recombinant technology, and confocal microscopy, we further demonstrated that the nonstructural viral NSs protein interacts with the p44 component of TFIIH to form nuclear filamentous structures that also contain XPB subunit of TFIIH. By competing with XPD, the natural partner of p44 within TFIIH, and sequestering p44 and XPB subunits, NSs prevents the assembly of TFIIH subunits, thus destabilizing the normal host cell life. These observations shed light on the mechanism utilized by RVFV to evade the host response.},
author = {{Le May}, Nicolas and Dubaele, Sandy and {De Santis}, Luca Proietti and Billecocq, Agn\`{e}s and Bouloy, Mich\`{e}le and Egly, Jean Marc},
doi = {10.1016/S0092-8674(04)00132-1},
isbn = {0092-8674 (Print)$\backslash$r0092-8674 (Linking)},
issn = {00928674},
journal = {Cell},
pages = {541--550},
pmid = {14980221},
title = {{TFIIH Transcription Factor, a Target for the Rift Valley Hemorrhagic Fever Virus}},
volume = {116},
year = {2004}
}
@article{Shi2014,
abstract = {The recent emergence of the H7N9 avian influenza A virus and its ability to infect humans emphasize the epidemic and pandemic potential of these viruses. Interspecies transmission is the result of many factors, which ultimately lead to a change in the host tropism of the virus. One of the key factors involved is a shift in the receptor-binding specificity of the virus, which is mostly determined by mutations in the viral haemagglutinin (HA). In this Review, we discuss recent crystallographic studies that provide molecular insights into HA-host receptor interactions that have enabled several influenza A virus subtypes to 'jump' from avian to human hosts.},
author = {Shi, Yi and Wu, Ying and Zhang, Wei and Qi, Jianxun and Gao, George F},
file = {:Users/Masha/Documents/Thesis/Reading/Avian flu/Host\_jump\_Nature.pdf:pdf},
