@article{Tisoncik2011,
abstract = {The NS1 protein of influenza virus counters host antiviral defences primarily by antagonizing the type I interferon (IFN) response. Both the N-terminal dsRNA-binding domain and the C-terminal effector domain are required for optimal suppression of host responses during infection. To better understand the regulatory role of the NS1 effector domain, we used an NS1-truncated mutant virus derived from human H1N1 influenza isolate A/Texas/36/91 (Tx/91) and assessed global transcriptional profiles from two independent human lung cell-culture models. Relative to the wild-type Tx/91-induced gene expression, the NS1 mutant virus induced enhanced expression of innate immune genes, specifically NF-kappaB signalling-pathway genes and IFN-alpha and -beta target genes. We queried an experimentally derived IFN gene set to gauge the proportion of IFN-responsive genes that are suppressed specifically by NS1. We show that the C-terminally truncated NS1 mutant virus is less efficient at suppressing IFN-regulated gene expression associated with activation of antigen-presentation and immune-proteasome pathways. This is the first report integrating genomic analysis from two independent human culture systems, including primary lung cells, using genetically similar H1N1 influenza viruses that differ only in the length of the NS1 protein.},
author = {Tisoncik, Jennifer R and Billharz, Rosalind and Burmakina, Svetlana and Belisle, Sarah E and Proll, Sean C and Korth, Marcus J and Garcia-Sastre, Adolfo and Katze, Michael G},
doi = {10.1099/vir.0.032060-0},
file = {:Users/Masha/Documents/Thesis/Reading/NS1\_pretranscriptional/2093.full.pdf:pdf},
issn = {1465-2099 (Electronic)},
journal = {Journal of General Virology},
keywords = {Antigen Presentation,Cells,Cultured,Epithelial Cells,Gene Expression Profiling,H1N1 Subtype,Humans,Influenza A Virus,Interferon-alpha,Interferon-beta,Mutant Proteins,NF-kappa B,Proteasome Inhibitors,Viral Nonstructural Proteins,Virulence Factors,antagonists \& inhibitors,genetics,immunology,metabolism,virology},
language = {eng},
month = sep,
number = {Pt 9},
pages = {2093--2104},
pmid = {21593271},
title = {{The NS1 protein of influenza A virus suppresses interferon-regulated activation of antigen-presentation and immune-proteasome pathways.}},
